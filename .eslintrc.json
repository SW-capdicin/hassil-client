{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
    "no-console": "warn",
    "no-var": "warn",
    "import/no-unresolved": "off"
  }
}
